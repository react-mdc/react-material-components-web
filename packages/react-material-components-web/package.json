{
  "name": "react-material-components-web",
  "description": "React Components for Metarial Component Web",
  "author": "Choi Geonu",
  "version": "0.1.14",
  "license": "MIT",
  "keywords": [
    "material components",
    "material design",
    "react"
  ],
  "main": "src/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-mdc/react-material-components-web.git"
  },
  "dependencies": {
    "@react-mdc/base": "^0.1.9",
    "@react-mdc/button": "^0.1.5",
    "@react-mdc/card": "^0.1.6",
    "@react-mdc/checkbox": "^0.1.9",
    "@react-mdc/dialog": "^0.1.4",
    "@react-mdc/drawer": "^0.1.10",
    "@react-mdc/elevation": "^0.1.6",
    "@react-mdc/fab": "^0.1.6",
    "@react-mdc/form-field": "^0.1.5",
    "@react-mdc/layout-grid": "^0.1.6",
    "@react-mdc/list": "^0.1.6",
    "@react-mdc/radio": "^0.1.9",
    "@react-mdc/ripple": "^0.1.8",
    "@react-mdc/switch": "^0.1.6",
    "@react-mdc/textfield": "^0.1.7",
    "@react-mdc/theme": "^0.1.5",
    "@react-mdc/toolbar": "^0.1.8",
    "@react-mdc/typography": "^0.1.5",
    "@types/classnames": "0.0.32",
    "@types/react": "^15.0.18"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.0",
    "@types/jest": "^19.2.3",
    "enzyme": "^2.8.2",
    "jest": "^20.0.1",
    "npm-run-all": "^4.0.2",
    "react-test-renderer": "^15.5.4",
    "shelljs": "^0.7.7",
    "shx": "^0.2.2",
    "ts-jest": "^20.0.3",
    "tslint": "^5.2.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.4"
  },
  "scripts": {
    "build": "npm-run-all build:ts lint",
    "watch": "tsc --watch",
    "prepublish": "npm run build",
    "clean": "shx rm -rf lib/",
    "lint": "tslint 'src/**/*.ts?(x)'",
    "build:ts": "tsc",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/.*(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
